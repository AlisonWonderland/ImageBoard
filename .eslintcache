ck}>\r\n                <img className=\"thumbnail\" src={fileInfo.thumbnailURL} alt=\"\"style={{display: showThumbnail ? \"\": \"none\"}} />\r\n                {fullSizeContent}\r\n            </a>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default File","/mnt/d/Users/dimitri/Desktop/react/photo_board/src/components/PostText.js",["131"],"import DOMPurify from 'dompurify'\r\n\r\nconst PostText = ({ text }) => {\r\n    // first figure out how to show newlines\r\n    // const test = '<a href=\"#1\"> >>12 </a>'\r\n    return (\r\n        <blockquote className=\"postMessage\" cite=\"\">\r\n            {/* <div dangerouslySetInnerHTML={{__html: DOMPurify.sanitize(test)}}></div> */}\r\n            {text}\r\n        </blockquote>\r\n    )\r\n}\r\n\r\nexport default PostText","/mnt/d/Users/dimitri/Desktop/react/photo_board/src/components/PostInfo.js",[],"/mnt/d/Users/dimitri/Desktop/react/photo_board/src/components/ReplyList.js",[],"/mnt/d/Users/dimitri/Desktop/react/photo_board/src/components/helpers.js",[],"/mnt/d/Users/dimitri/Desktop/react/photo_board/src/components/contexts/index.js",[],"/mnt/d/Users/dimitri/Desktop/react/photo_board/src/components/contexts/FormContext.js",[],"/mnt/d/Users/dimitri/Desktop/react/photo_board/src/components/CommentLink.js",["132"],"import React, { useContext } from 'react'\r\nimport { FormContext } from './contexts'\r\n\r\nconst CommentLink = () => {\r\n    const { handleFormOpen, parentThread } = useContext(FormContext)\r\n\r\n    const handleClick = (e) => {\r\n        e.preventDefault();\r\n        handleFormOpen(parentThread, 'thread', false)\r\n    }\r\n\r\n    console.log('checking')\r\n\r\n    return (\r\n            <span>\r\n                [ <a href=\"#\" onClick={handleClick}>Post a comment</a> ]\r\n            </span>\r\n    )\r\n}\r\n\r\nexport default CommentLink","/mnt/d/Users/dimitri/Desktop/react/photo_board/src/components/Textarea.js",[],"/mnt/d/Users/dimitri/Desktop/react/photo_board/src/components/Row.js",[],"/mnt/d